datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
  directUrl         = env("DIRECT_URL")
  schemas           = ["public", "auth"]
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["multiSchema"]
}



model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
  dateBirth String?
  country String?
  oshimen String[]
  imgUrl String?
   @@schema("public") // or @@schema("public")
}

